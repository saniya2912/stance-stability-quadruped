# CompilerName=lcc64
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root\build\win64
matlabdir = C:\Program Files\MATLAB\R2021a
startdir = C:\Users\Saniya\OneDrive\Documents\Robotics\Delhi\Quadruped robot Simmechanics\btp_alinjar

# Toolchain information
cc = lcc64
cxx = 
linker = lcclnk64
linkerxx = 

cflags =  -nodeclspec -Zp8 -dll -c -I"$matlabdir\sys\lcc64\lcc64\include64" -DMATLAB_MEX_FILE -noregistrylookup -DNDEBUG -I"$root" -I"$startdir" -I"$root\interface" -I"$matlabdir\extern\include" -I"." -DMODEL=findh_mex  

cxxflags =  -I"$root" -I"$startdir" -I"$root\interface" -I"$matlabdir\extern\include" -I"." -DMODEL=findh_mex  

ldflags =  -dll -L"$matlabdir\sys\lcc64\lcc64\lib64" -L"$matlabdir\extern\lib\win64\microsoft" -entry LibMain libmx.lib libmex.lib libmat.lib libemlrt.lib libcovrt.lib libut.lib libmwmathutil.lib -o findh_mex.mexw64 -s  findh_mex.def

# Build Rules
rule cc
    command = $cc $cflags $in -Fo$out

rule cxx
    command = $cxx $cxxflags $in -Fo$out

rule link
    command = $linker $in $ldflags

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir\findh_data.obj : cc $root\findh_data.c
build $builddir\rt_nonfinite.obj : cc $root\rt_nonfinite.c
build $builddir\findh_initialize.obj : cc $root\findh_initialize.c
build $builddir\findh_terminate.obj : cc $root\findh_terminate.c
build $builddir\findh.obj : cc $root\findh.c
build $builddir\_coder_findh_api.obj : cc $root\interface\_coder_findh_api.c
build $builddir\_coder_findh_mex.obj : cc $root\interface\_coder_findh_mex.c
build $builddir\_coder_findh_info.obj : cc $root\interface\_coder_findh_info.c
build $builddir\c_mexapi_version.obj : cc $matlabdir\extern\version\c_mexapi_version.c
build $builddir\lccstub.obj : cc $matlabdir\sys\lcc64\lcc64\mex\lccstub.c

# Link
build $root\findh_mex.mexw64 : link $builddir\findh_data.obj $builddir\rt_nonfinite.obj $builddir\findh_initialize.obj $builddir\findh_terminate.obj $builddir\findh.obj $builddir\_coder_findh_api.obj $builddir\_coder_findh_mex.obj $builddir\_coder_findh_info.obj $builddir\c_mexapi_version.obj $builddir\lccstub.obj 
